#include<stdio.h>
#include<stdlib.h>
#include<string.h>
int main(int argc,char* argv[])
{
	int sum = 0;
	int num1 = atoi(argv[2]);
	int num2 = atoi(argv[3]);
	if(strcmp(argv[1],"-a")==0)
	{
		printf("%d\n",num1+num2);
	}
	else if(strcmp(argv[1],"-s")==0)
	{
                printf("%d\n",num1-num2);		
	}
        else if((strcmp(argv[1],"-m")==0))
	{
                printf("%d\n",num1*num2);
	}
        else if((strcmp(argv[1],"-d")==0))
	{
                printf("%d\n",num1/num2);
	}
	return 0;
}
