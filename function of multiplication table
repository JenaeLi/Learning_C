#include<stdio.h>
void Mul_Tab(int input)
{
	int i = 0;
	int j = 0;
	int mul = 0;
	for(i = 1;i <= input;i++)
	{
		for(j = 1;j <= i;j++)
		{
			mul = i*j;
			printf(" %d*%d = %d  ",i,j,mul);
		}
		printf("\n");
	}
}
int main()
{
	int n = 0;
	printf("Please input n:\n");
	scanf("%d",&n);
	Mul_Tab(n);
	return 0;
}
