#define _CRT_SECURE_NO_WARNINGS 1

#include<stdio.h>

int main()
{
	int a = 20;
	int b = 36;
	int i = 0;
	int min = 0;
	int ret = 0;
	if(a < b)
	{
		min = a;
	}
	else
	{
		min = b;
	}
	for(i = 1;i <= min;i++)
	{
		if(a % i == 0)
		{
			if(b % i == 0)
			{
				ret = i;
			}
		}
	}
	printf("%d\n",ret);
	return 0;
}

