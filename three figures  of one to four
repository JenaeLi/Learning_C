#include<stdio.h>
int main()
{
	int i = 0;
	int j = 0;
	int k = 0;
	int count = 0;
	for(i = 1;i <= 4;i++)
	{
		for(j = 1;j <= 4;j++)
		{
			for(k = 1;k <= 4;k++)
			{
				if(i == j&&i == k)
				{
					;
				}
				else
				{
					printf("%d ",i*100+j*10+k);
					count++;
				}
			}
		}
	}
	printf("\n");
	printf("%d\n",count);
	return 0;
}
