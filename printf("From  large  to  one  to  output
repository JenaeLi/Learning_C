#define _CRT_SECURE_NO_WARNINGS 1

#include<stdio.h>


int main()
{
	int a = 11;
	int b = 15;
	int c = 13;
	printf("From  large  to  one:\n");
	if(a > b)
	{
		if(b > c)
		{
			printf("%d %d %d\n",a,b,c);
		}
		else
		{
			if(a > c)
			{
				printf("%d %d %d\n",a,c,b);
			}
			else
			{
				printf("%d %d %d\n",c,a,b);
			}
		}
	}
	else
	{
		if(a > c)
		{
			printf("%d %d %d\n",b,a,c);
		}
		else
		{
			if(b > c)
			{
				printf("%d %d %d\n",b,c,a);
			}
			else
			{
				printf("%d %d %d\n",c,b,a);
			}
		}

	}
	return 0;
}
