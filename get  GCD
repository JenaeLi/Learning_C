#include<stdio.h>
int main()
{
	int a = 54;
	int b = 36;
	int r = 1;
	printf("GCDï¼š\n");
	while(r != 0)
	{
		r = a%b;
		a = b;
		b = r;
	}
	printf("%d\n",a);
	return 0;
}
