#include<stdio.h>
int main()
{
	int arr[] = {1,3,5,7,1,3,5};
	int sz = sizeof(arr)/sizeof(arr[0]);
	int i = 0;
	int j = 0;
	for(i = 0;i < sz;i++)
	{
		for(j = 0;j < sz;j++)
		{
			if((i!=j)&&(arr[i] ^ arr[j]) == 0)
			{
				arr[j] = 0;
				break;
			}
		}
		if(j == sz)
		{
			printf("%d ",arr[i]);
		}
	}
	printf("\n");
	return 0;
}
